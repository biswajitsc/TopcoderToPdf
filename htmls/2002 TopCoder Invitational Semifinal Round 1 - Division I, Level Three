               <tr>
                <td class="statText">
                 Class:
                </td>
                <td class="statText">
                 HigherMaze
                </td>
               </tr>
               <tr>
                <td class="statText">
                 Method:
                </td>
                <td class="statText">
                 navigate
                </td>
               </tr>
               <tr>
                <td class="statText">
                 Parameters:
                </td>
                <td class="statText">
                 int, int, int[], int[], int[], String[]
                </td>
               </tr>
               <tr>
                <td class="statText">
                 Returns:
                </td>
