            <tr>
             <td align="center" class="statText" nowrap="true">
              0)
             </td>
             <td class="statText">
             </td>
            </tr>
            <tr>
             <td class="statText">
             </td>
             <td class="statText">
              <table>
               <tr>
                <td class="statText">
                 <table>
                  <tr>
                   <td class="statText">
                    <pre>{1,1,2,2,9,3}</pre>
                   </td>
